# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~




#Community
GET     /                               Application.index
GET     /yaml/{communityName}           Application.yaml
GET     /community/{communityName}      CommunityController.index

#Portfolio
GET     /community/{communityName}/portfolio/{portfolioName}  PortfolioController.index
GET     /community/{communityName}/edit/{portfolioName}  PortfolioController.edit

#Quote
GET     /community/{communityName}/quote/{quoteName}  PortfolioController.index

# Import the default CRUD routes
*			/admin						module:crud


# Map static resources from the /app/public folder to the /public path
GET     /public/                                staticDir:public

# Catch all
*       /{controller}/{action}                  {controller}.{action}
